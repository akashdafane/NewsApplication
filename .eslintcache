ormReducer\",(valuesTosave))\n    //    })\n\n    return (\n        <form onSubmit={(e) => {\n            e.preventDefault();\n            // props.dispatch(addInfo(name.firstName,name.lastName,name.email,name.phone,name.address,name.DOB,name.time,name.url));\n            props.dispatch(addInfo(\n                firstName.firstName,\n                lastName.lastName,\n                email.email,\n                phone.phone,\n                address.address,\n                dob.DOB,\n                time.time,\n                url.url\n            ));\n        }}>\n            <div className=\"container-fluid mt-2\">\n                <div className=\"row\">\n                    {/* firstName */}\n                    <div className=\"col-md-6\">\n                        <label>First Name</label>\n                        <input\n                            type=\"text\"\n                            value={firstName.firstName}\n                            className=\"form-control\"\n                            onChange={e => setFname({ firstName: e.target.value })}\n                        />\n                    </div>\n\n                    {/* lastName */}\n\n                    <div className=\"col-md-6\">\n                        <label>Last Name</label>\n                        <input\n                            type=\"text\"\n                            className=\"form-control\"\n                            value={lastName.lastName}\n                            onChange={e => setLname({ lastName: e.target.value })}\n                        />\n                    </div>\n\n                    {/* email */}\n                    <div className=\"col-md-6\">\n                        <label>Email</label>\n                        <input\n                            type=\"email\"\n                            className=\"form-control\"\n                            value={email.email}\n                            onChange={e => setEmail({ email: e.target.value })}